<h1>Listing cloud_record_summaries</h1>

<table>
  <tr>
    <th>Date</th>
    <th>Site</th>
    <th>Local group</th>
    <th>Local user</th>
    <th>Vmcount</th>
    <th>Wallduration</th>
    <th>Networkinbound</th>
    <th>Networkoutbound</th>
    <th>Cpucount</th>
    <th>Memory</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cloud_record_summaries.each do |cloud_record_summary| %>
  <tr>
    <td><%= cloud_record_summary.date %></td>
    <td><%= cloud_record_summary.site_id %></td>
    <td><%= cloud_record_summary.local_group %></td>
    <td><%= cloud_record_summary.local_user %></td>
    <td><%= cloud_record_summary.vmCount %></td>
    <td><%= cloud_record_summary.wallDuration %></td>
    <td><%= cloud_record_summary.networkInBound %></td>
    <td><%= cloud_record_summary.networkOutBound %></td>
    <td><%= cloud_record_summary.cpuCount %></td>
    <td><%= cloud_record_summary.memory %></td>
    <td><%= link_to 'Show', cloud_record_summary %></td>
    <td><%= link_to 'Edit', edit_cloud_record_summary_path(cloud_record_summary) %></td>
    <td><%= link_to 'Destroy', cloud_record_summary, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

