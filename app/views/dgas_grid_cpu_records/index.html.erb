    <th>Uniquechecksum</th>
    <th>Dgjobid</th>
    <th>Startdate</th>
    <th>Enddate</th>
    <th>Resource</th>
    <th>Globalusername</th>
    <th>Fqan</th>
    <th>Uservo</th>
    <th>Cputime</th>
    <th>Walltime</th>
