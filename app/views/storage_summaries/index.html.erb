<h1>Listing storage_summaries</h1>

<table>
  <tr>
    <th>Date</th>
    <th>Publisher</th>
    <th>Site</th>
    <th>Storagesystem</th>
    <th>Group</th>
    <th>Resourcecapacityused</th>
    <th>Logicalcapacityused</th>
    <th>Storageshare</th>
    <th>ResourceCapacityAllocated</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @storage_summaries.each do |storage_summary| %>
  <tr>
    <td><%= storage_summary.date %></td>
    <td><%= storage_summary.publisher_id %></td>
    <td><%= storage_summary.site %></td>
    <td><%= storage_summary.storageSystem %></td>
    <td><%= storage_summary.group %></td>
    <td><%= storage_summary.resourceCapacityUsed %></td>
    <td><%= storage_summary.logicalCapacityUsed %></td>
    <td><%= storage_summary.storageShare %></td>
    <td><%= storage_summary.resourceCapacityAllocated %></td>
    <td><%= link_to 'Show', storage_summary %></td>
    <td><%= link_to 'Edit', edit_storage_summary_path(storage_summary) %></td>
    <td><%= link_to 'Destroy', storage_summary, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

