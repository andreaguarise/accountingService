<h1>Listing database_tables</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Database scheme</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @database_tables.each do |database_table| %>
  <tr>
    <td><%= database_table.name %></td>
    <td><%= database_table.database_scheme_id %></td>
    <td><%= link_to 'Show', database_table %></td>
    <td><%= link_to 'Edit', edit_database_table_path(database_table) %></td>
    <td><%= link_to 'Destroy', database_table, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

