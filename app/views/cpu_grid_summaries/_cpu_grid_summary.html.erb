<tr class="<%= cycle("odd", "even") %>">
    <td><%= cpu_grid_summary.date %></td>
    <td><%= if cpu_grid_summary.site then cpu_grid_summary.site.name end%></td>
    <td><%= if cpu_grid_summary.publisher then cpu_grid_summary.publisher.hostname end%></td>
    <td><%= cpu_grid_summary.globalUserName %></td>
    <td><%= cpu_grid_summary.vo %></td>
    <td><%= cpu_grid_summary.fqan %></td>
    <td><%= cpu_grid_summary.records %></td>
    <td><%= cpu_grid_summary.cpuDuration %></td>
    <td><%= cpu_grid_summary.wallDuration %></td>
    <td><%= cpu_grid_summary.benchmark_value %></td>
    <td><%= if cpu_grid_summary.benchmark_type then cpu_grid_summary.benchmark_type.name end%></td>
    <td><%= link_to 'Show', cpu_grid_summary %></td>    
  </tr>